@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using ClientDependency.Core.Mvc
@using ClientDependency.Core
@{
  var seo = CurrentPage._seoFields;
  var current = umbraco.NodeFactory.Node.GetCurrent();
  string metaDescription = @seo.description;
  string title = @seo.title;
  string analyticsID = System.Configuration.ConfigurationManager.AppSettings["GoogleAnalyticsAccountId"];
  string type = "website";
  string websiteurl = System.Configuration.ConfigurationManager.AppSettings["ICSBaseURL"];
  string schemaauthor = "ICS Learn";
  string twiiterauthor = "@ICSLearn";
  string ABurl = System.Configuration.ConfigurationManager.AppSettings["ICSBaseURL"] + Model.Content.Url;
  var image = Model.Content.GetPropertyValue("smartBlogImage");
  string result = Convert.ToString(image);
  var PostIDAMP = Umbraco.Media(result);
  var PostImageAMP = PostIDAMP.Url;
    Html.RequiresJs("~/scripts/bootstrap.min.js", 1);
    Html.RequiresJs("~/scripts/wow.js", 2);
    Html.RequiresJs("~/scripts/carousel.js", 3);
    Html.RequiresJs("~/scripts/carousel.js", 4);
    Html.RequiresJs("~/scripts/custom.js", 5);
    Html.RequiresJs("~/scripts/jquery.themepunch.tools.min.js", 6);
    Html.RequiresJs("~/scripts/jquery.themepunch.revolution.min.js", 7);
    Html.RequiresJs("~/scripts/bxslider.js", 8);
    Html.RequiresJs("~/scripts/CourseGuideJavaScript.js", 9);
    Html.RequiresJs("~/scripts/analyticswebsite.js", 10);
    Html.RequiresJs("~/scripts/settings.js", 11);
    
    Html.RequiresCss("~/css/style.css", 1);
    Html.RequiresCss("~/css/settings.css", 2);
    Html.RequiresCss("~/css/RTE.css", 3);
}

